vector<int> Solution::solve(vector<int> &A) {
    int n = A.size();
    int i = 0;
    int j = n - 1;
    int k = n - 1;
    vector<int> soln(n);
    while (i < j) {
        if (abs(A[i]) > A[j]) {
            soln[k] = A[i] * A[i];
            i++;
        } else {
            soln[k] = A[j] * A[j];
            j--;
        }
        k--;
    }
    return soln;
}
